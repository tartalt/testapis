type Query{
    allPatients: [Patient],
    patientById(id: ID!): Patient
}

type Mutation{
    savePatient(patient: PatientDto): Patient
}
type Patient{
    id: Int,
    name: String ,
    email: String
    description_diagnostic: String
}
input PatientDto{
    name: String,
    email: String
    description_diagnostic: String
}